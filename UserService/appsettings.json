{
   "Eureka": {
      "Client": {
         "ServiceUrl": "http://localhost:8761/eureka/"
      },
      "Instance": {
         "AppName": "UserService",
         "InstanceId": "UserService-${random.value}",
         "PreferIpAddress": true,
         "LeaseRenewalIntervalInSeconds": 30,
         "LeaseExpirationDurationInSeconds": 90
      }
   },
   "Logging": {
      "LogLevel": {
         "Default": "Information",
         "Microsoft.AspNetCore": "Warning"
      }
   },
   "AllowedHosts": "*"
}
